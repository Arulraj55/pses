# Copy to .env and fill in values. Do NOT commit .env.

# --- APIs ---
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash-latest
YOUTUBE_API_KEY=
# Images (Google Custom Search)
GOOGLE_CSE_API_KEY=
GOOGLE_CSE_CX=

# --- Firebase (client-side config; still keep it out of git for consistency) ---
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# --- Google Analytics (optional gtag on client) ---
VITE_GA_MEASUREMENT_ID=

# --- Services ---
VITE_API_BASE_URL=http://localhost:5000
ML_SERVICE_URL=http://127.0.0.1:8000

# --- Firebase Admin (server) ---
# Option A (recommended): paste full service account JSON as ONE LINE (escape newlines)
FIREBASE_SERVICE_ACCOUNT_JSON=
# Option B: point to service account file (Application Default Credentials)
GOOGLE_APPLICATION_CREDENTIALS=

# --- Database ---
MONGODB_URI=mongodb://127.0.0.1:27017/pses

# --- Server ---
PORT=5000
CORS_ORIGIN=http://localhost:5173
